{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: FastAPI",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "app.main:app",
                "--host=127.0.0.1",
                "--port=8080",
                "--reload",
                "--log-level=debug",
            ],
            "jinja": true,
            "justMyCode": true,
            "cwd": "${workspaceFolder}/src/",
            "envFile": "${workspaceFolder}/.env"
        }
    ]
}
